{
  "expo": {
    "name": "hymnraise",
    "slug": "hymnraise",
    "scheme": "your-app-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/group44.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8B5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash1.png",
        "backgroundColor": "#FFF8B5"
      }
    },
    "web": {
      "favicon": "./assets/framesplash.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
